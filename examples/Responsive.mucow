<?xml version="1.0" encoding="UTF-8"?>
<!--
Sample Responsive HTML Widget that displays a blue box
-->
<HTMLWidget name="BlueBox" formatNumber="4" localization="none" creator="Adobe Muse Team" defaultWidth="800" defaultHeight="330" isResizable="true" isResponsive="true">
	<parameters>
		<builtIn name="height" />
	</parameters>
	<pageItemHTML>
<![CDATA[
  <div style="background-color: dodgerblue; height: {param_height}px; width: 100%;"> </div>
]]>
	</pageItemHTML>
	
</HTMLWidget>
